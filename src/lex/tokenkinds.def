#ifndef TOK
#define TOK(X)
#endif

#ifndef LITERAL
#define LITERAL(X) TOK(X)
#endif

#ifndef PUNCT
#define PUNCT(X,Y) TOK(X)
#endif

#ifndef UPUNCT
#define UPUNCT(NM, SYM) PUNCT(NM, SYM)
#endif

#ifndef BPUNCT
#define BPUNCT(NM, SYM, PREC) PUNCT(NM, SYM)
#endif

#ifndef UBPUNCT
#define UBPUNCT(NM, SYM, BPREC) PUNCT(NM, SYM)
#endif

#ifndef KEYWORD
#define KEYWORD(X) TOK(kw_ ## X)
#endif

#ifndef TYPE
#define TYPE(X) KEYWORD(X)
#endif

//basic type
#ifndef BTYPE
#define BTYPE(X,SZ,SIGN) TYPE(X)
#endif

//floating point
#ifndef FTYPE
#define FTYPE(X,SZ) TYPE(X)
#endif

//vector types
#ifndef VTYPE
#define VTYPE(X,LEN,TYPE) TYPE(X)
#endif

#ifndef RESERVE
#define RESERVE(X) TOK(resv_ ## X)
#endif

//matrix types?

TOK(none)
TOK(eof)
TOK(newline)
TOK(whitespace)
TOK(unknown)
TOK(comment)
TOK(identifier)
LITERAL(floatlit)
